{
  "name": "@intellicar/phoenix-design-system",
  "version": "0.1.11",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.10"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public --no-manager-cache",
    "build-storybook": "build-storybook -s public",
    "release": "auto shipit --base-branch=main"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addons": "^6.3.2",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.2",
    "@storybook/theming": "^6.3.2",
    "auto": "^10.29.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "sass": "^1.34.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0"
  },
  "description": "The Intellicar Design System was created to help us build products for our customers more efficiently.",
  "main": "dist/index.js",
  "repository": "git@github.com:intellicar/phoenix-design-system.git",
  "author": "sunny.sharma@intellicar.in",
  "license": "MIT"
}
